{
  "appenders": {
    "console":{
      "type": "stdout"      
    },
    "file": {
      "type": "file",
      "filename": "log/dotup.ipc.log",
      "maxLogSize": 10485760,
      "numBackups": 3
    }
  },
  "categories": {
    "debug": {
      "appenders": [
        "console"
      ],
      "level": "DEBUG"
    },
    "default": {
      "appenders": [
        "file",
        "console"
      ],
      "level": "INFO"
    },
    "IPC": {
      "appenders": [
        "file"
      ],
      "level": "INFO"
    }
  }
}